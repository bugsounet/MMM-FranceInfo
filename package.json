{
  "name": "MMM-FranceInfo",
  "version": "1.4.1",
  "rev": "241120",
  "description": "France Info Rss Reader",
  "main": "MMM-FranceInfo.js",
  "scripts": {
    "preinstall": "installer/preinstall.sh",
    "postinstall": "installer/postinstall.sh -m",
    "update": "installer/update.sh",
    "reset": "rm -f *.js && rm -rf components && git reset --hard",
    "clean": "rm -rf node_modules package-lock.json",
    "lint": "eslint . --config eslint.config.mjs",
    "lint:fix": "eslint . --config eslint.config.mjs --fix",
    "test": "npm run lint",
    "test:minify": "cd installer && node minify.js",
    "dev": "cd installer && node dev.js",
    "dev:src": "cd installer && node src.js"
  },
  "keywords": [
    "magic mirror",
    "module",
    "news",
    "french",
    "franceinfo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/MMM-FranceInfo"
  },
  "author": "@bugsounet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugsounet/MMM-FranceInfo/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "homepage": "https://github.com/bugsounet/MMM-FranceInfo",
  "dependencies": {
    "esbuild": "^0.24.0",
    "feedme": "^2.0.2",
    "glob": "^11.0.0",
    "stream": "^0.0.3"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.11.0",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-package-json": "^0.18.0"
  }
}
